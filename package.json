    "uglify-js": "^3.14.5",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "worker-loader": "^3.0.8"
  }
}
