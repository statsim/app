    "css-loader": "^6.5.1",
    "electron": "^16.0.5",
    "electron-debug": "^3.0.0",
    "electron-packager": "^15.4.0",
    "http-server": "^14.0.0",
    "jest": "^29.5.0",
    "jest-puppeteer": "^8.0.6",
    "node-sass": "^9.0.0",
    "nodemon": "^2.0.22",
    "puppeteer": "^20.3.0",
