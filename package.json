{
  "name": "statsim",
  "version": "0.12.1",
  "description": "Web app for statistical simulations and bayesian inference",
  "main": "electron.js",
  "private": true,
  "scripts": {
    "_build": "npm run build-web && npm run build-bin",
    "_build-web": "browserify src/main.js -t [ vueify ] -t [ babelify ] -g [ envify --NODE_ENV production ] | uglifyjs -cm > dist/bundle.js",
    "_build-bin": "rm -r bin && mkdir bin && electron-packager . --ignore=\"(node_modules|src)\"  --platform linux,win32 --out bin && cd ./bin && for i in */; do zip -r \"${i%/}.zip\" \"$i\"; done",
    "_build-dev": "browserify src/main.js -t [ vueify ] -o dist/bundle.js --debug",
    "_watch": "nodemon --watch src --ext vue,js,css,html --exec 'npm run build-dev'",
    "_electron": "electron .",
    "build-dev": "webpack --mode=development --progress --stats-children --env DEVELOPMENT",
    "build": "webpack --mode=production --progress",
    "watch": "nodemon --watch . --ignore dist --ext vue,js,css,html --exec 'npm run build-dev'",
    "test": "http-server -s -p 8085 & tape test.js | tap-summary && kill $!"
  },
  "author": "Anton Zemlyansky",
  "license": "ISC",
  "dependencies": {
    "buffer": "^6.0.3",
    "chaikin-smooth": "^1.0.4",
    "cookie": "^0.3.1",
    "crypto-browserify": "^3.12.0",
    "csv-parse": "^5.0.4",
    "csv-parser": "^1.12.1",
    "d3-array": "^1.2.4",
    "d3-hist2d": "^1.0.7",
    "dat-js": "^7.1.1",
    "density-plot": "^1.0.4",
    "dygraphs": "^2.1.0",
    "esprima": "^4.0.1",
    "file-saver": "^2.0.0",
    "filestream": "^4.1.3",
    "flat": "^4.1.0",
    "handsontable": "^11.0.1",
    "html-escape": "^2.0.0",
    "https-browserify": "^1.0.0",
    "is": "^3.2.1",
    "is-json": "^2.0.1",
    "js-beautify": "^1.8.9",
    "js-quantities": "^1.7.2",
    "json2csv": "^4.5.1",
    "online-stats": "^1.3.0",
    "path-browserify": "^1.0.1",
    "query-string": "^6.2.0",
    "split": "^1.0.1",
    "stream": "0.0.2",
    "stream-filter": "^2.1.0",
    "stream-http": "^3.2.0",
    "streamsaver": "^1.2.0",
    "through2": "^3.0.1",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "vue": "^2.6.14",
    "vue-color": "^2.7.0",
    "vue-material": "^0.8.1",
    "vue-style-loader": "^4.1.3",
    "vue2vis": "0.0.15",
    "vuedraggable": "^2.17.0",
    "webtorrent": "^1.5.8",
    "xml-nodes": "^0.1.5",
    "xml-objects": "^1.0.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "electron": "^16.0.5",
    "electron-debug": "^3.0.0",
    "electron-packager": "^15.4.0",
    "http-server": "^14.0.0",
    "jest": "^27.4.5",
